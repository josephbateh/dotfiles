{
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "editor.defaultFormatter": "charliermarsh.ruff",
        "editor.formatOnSave": true,
        "editor.wordBasedSuggestions": "off"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "chat.mcp.discovery.enabled": {
        "claude-desktop": false,
        "windsurf": false,
        "cursor-global": false,
        "cursor-workspace": false
    },
    "cursor.composer.shouldChimeAfterChatFinishes": true,
    "cursor.composer.usageSummaryDisplay": "always",
    "cursor.cpp.disabledLanguages": [
        "scminput"
    ],
    "cursorpyright.analysis.autoImportCompletions": true,
    "datadog.staticAnalysis.notification.suppressOnboarding": true,
    "datadog.telemetry.setup.enableTelemetry": "enabled",
    "datadog.telemetry.setup.suppressMigrationNotification": true,
    "diffEditor.experimental.showMoves": true,
    "diffEditor.maxComputationTime": 0,
    "diffEditor.renderSideBySide": true,
    "editor.minimap.enabled": false,
    "editor.rulers": [
        120
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmPasteNative": false,
    "extensions.verifySignature": true,
    "git-graph.dialog.resetCurrentBranchToCommit.mode": "Soft",
    "git-graph.repository.showRemoteBranches": false,
    "git.allowForcePush": true,
    "git.autoRepositoryDetection": "subFolders",
    "git.confirmForcePush": false,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "never",
    "githubPullRequests.pullPullRequestBranchBeforeCheckout": "never",
    "githubPullRequests.allowFetch": false,
    "github.copilot.nextEditSuggestions.enabled": true,
    "gitlab.duo.enabledWithoutGitlabProject": false,
    "gitlab.duoAgentPlatform.enabled": false,
    "gitlab.duoChat.enabled": false,
    "gitlab.duoCodeSuggestions.enabled": false,
    "gitlab.keybindingHints.enabled": false,
    "gitlab.real-timeSecurityScan.scanFileOnSave": false,
    "keyboard.touchbar.enabled": false,
    "makefile.configureOnOpen": true,
    "markdownlint.lintWorkspaceGlobs": [
        "**/*.{md,mkd,mdwn,mdown,markdown,markdn,mdtxt,mdtext,workbook}",
        "!**/*.code-search",
        "!**/bower_components",
        "!**/node_modules",
        "!**/.git",
        "!**/vendor",
        "!documentation"
    ],
    "prettier.proseWrap": "always",
    "python.analysis.autoImportCompletions": true,
    "python.analysis.indexing": true,
    "python.analysis.userFileIndexingLimit": 10000,
    "python.languageServer": "None",
    "python.terminal.activateEnvironment": false,
    "python.testing.autoTestDiscoverOnSaveEnabled": true,
    "python.testing.pytestEnabled": true,
    "redhat.telemetry.enabled": false,
    "scm.workingSets.default": "current",
    "scm.workingSets.enabled": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.scrollback": 10000,
    "testing.showAllMessages": true,
    "workbench.colorTheme": "Darcula",
    "workbench.editor.mouseBackForwardToNavigate": false,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.preventPinnedEditorClose": "never",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "yaml.format.printWidth": 120,
    "yaml.maxItemsComputed": 5000,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "application.shellEnvironmentResolutionTimeout": 20,
    "chat.tools.terminal.autoApprove": {
        "peach --help": {
            "approve": true,
            "matchCommandLine": true
        },
        "peach check --lint --fix --vulture --deps --py-deps --fix-increases": {
            "approve": true,
            "matchCommandLine": true
        }
    },
    "githubPullRequests.pullBranch": "never",
    "git.enableSmartCommit": true,
    "githubPullRequests.queries": [
        {
            "label": "Copilot on My Behalf",
            "query": "repo:${owner}/${repository} is:open author:copilot involves:${user}"
        },
        {
            "label": "Local Pull Request Branches",
            "query": "default"
        },
        {
            "label": "Waiting For My Review",
            "query": "repo:${owner}/${repository} is:open review-requested:${user} sort:updated-desc "
        },
        {
            "label": "Created By Me",
            "query": "repo:${owner}/${repository} is:open author:${user}"
        },
        {
            "label": "All Open",
            "query": "default"
        }
    ],
    "editor.accessibilitySupport": "off",
    "markdownlint.config": {
        "MD033": false
    }
}